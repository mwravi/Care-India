    <style name="SpinnerView">
        <item name="android:gravity">left</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/_10sdp</item>
        <item name="android:layout_marginRight">@dimen/_10sdp</item>
        <item name="android:minHeight">@dimen/_30sdp</item>
        <!-- <item name="android:minHeight">@dimen/_30sdp</item>-->
        <item name="android:popupBackground">@color/grey</item>
        <item name="android:textSize">@dimen/_13sdp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">@color/black</item>

    </style>

    <style name="TextInputStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColorHint">@color/black</item>
        <item name="android:padding">@dimen/_5sdp</item>
        <item name="android:layout_marginStart">@dimen/_5sdp</item>
        <item name="android:layout_marginEnd">@dimen/_5sdp</item>
        <item name="android:background">@color/light_gray</item>
        <item name="android:layout_marginTop">8dp</item>
    </style>

    <style name="LinearLayoutStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
    </style>
    <style name="EditTextStyleOld">
        <item name="android:textSize">@dimen/_12ssp</item>
        <item name="android:textColor">@color/grey_text</item>
        <item name="android:gravity">center|start</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="hintTextColor">@color/grey_text</item>
        <item name="android:fontFamily">@font/firasans_regular</item>
        <item name="android:minHeight">@dimen/_35sdp</item>
    </style>
    <style name="TextViewStyleNew">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/_12ssp</item>
        <item name="android:textColor">@color/black_text</item>
        <item name="android:fontFamily">@font/firasans_regular</item>
        <item name="android:minHeight">@dimen/_20sdp</item>
        <item name="android:padding">@dimen/_2sdp</item>
    </style>

    <style name="TextViewTitleStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/_16ssp</item>
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:fontFamily">@font/firasans_regular</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="ButtonTextStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">@dimen/_12ssp</item>
        <item name="android:padding">@dimen/_6ssp</item>
        <item name="android:background">@drawable/button_bg</item>
    </style>

    <style name="SpinnerViewNew">
        <item name="android:gravity">left</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">@dimen/_35sdp</item>
        <item name="android:textSize">@dimen/_18ssp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">@color/grey_text</item>
        <item name="android:padding">@dimen/_3sdp</item>
        <item name="android:layout_marginStart">@dimen/_2sdp</item>
        <item name="android:layout_marginEnd">@dimen/_2sdp</item>
        <item name="android:fontFamily">@font/firasans_regular</item>
        <item name="ms_alignLabels">false</item>
        <item name="ms_arrowColor">@color/colorPrimaryDark</item>
        <item name="ms_baseColor">@color/border_grey</item>
        <item name="ms_arrowSize">12dp</item>
        <item name="ms_enableErrorLabel">false</item>
        <item name="ms_enableFloatingLabel">false</item>
        <item name="ms_multiline">false</item>
        <item name="ms_thickness">0.5dp</item>
    </style>

    <style name="radioGroupStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_marginTop">5dp</item>
    </style>


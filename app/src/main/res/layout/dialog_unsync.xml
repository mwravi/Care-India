<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <!-- layout title -->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="@dimen/_40sdp"
            android:background="@color/colorPrimaryDark">

            <TextView style="@style/NewSimpleTextMargin" />

            <TextView
                style="@style/NewSimpleTextNew"
                android:gravity="center"
                android:text="@string/unsynchronized_data"
                android:textAppearance="?android:attr/textAppearance"
                android:textColor="@color/colorPrimary" />

            <TextView style="@style/NewSimpleTextMargin" />

        </TableRow>
        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            >

            <TextView
                android:id="@+id/tvCheckrecords"
                style="@style/NewSimpleTextNew"
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:background="@drawable/sheet1"
                android:gravity="center_vertical" />

            <TextView
                style="@style/NewSimpleTextNew"
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:background="@drawable/sheet1"
                android:gravity="center"
                android:text="@string/unsynchronized"
                android:textColor="@color/white" />

            <TextView
                style="@style/NewSimpleTextNew"
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:background="@drawable/sheet1"
                android:gravity="center"
                android:text="@string/total"
                android:textColor="@color/white" />
        </TableRow>

        <ScrollView
            style="@style/ScrolllViewStyle"
            android:layout_weight="1"
            android:scrollbars="none">


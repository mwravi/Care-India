                android:layout_weight="1" />

            <Button
                android:id="@+id/btn_ok"
                style="@style/ButtonTextStyle"
                android:layout_width="0dp"
                android:layout_height="@dimen/_30sdp"
                android:layout_weight="1"
                android:text="@string/close" />

            <TextView
                style="@style/NewSimpleTextMargin"
                android:layout_weight="1" />


        </TableRow>
